            splitStatements="true"
            stripComments="true"/>
    </changeSet>

    <changeSet author="liquibase-docs" id="schema-1.0.3">
        <sqlFile 
            encoding="utf8"
            endDelimiter="\nGO"
            path="db/changelog/schema_restaurant.sql"
            splitStatements="true"
            stripComments="true"/>
    </changeSet>
    
    <include file="db/changelog/schema_menu/schema-create.xml" relativeToChangelogFile="false"/>
    
